{
  "id": "exiting",
  "title": "Children with Disabilities (IDEA) Exiting",
  "metaTags": ["Part B", "Exiting"],
  "reportingLevels": ["SEA (State Education Agency)", "LEA (Local Education Agency)"],
  "suppression": {
    "required": true,
    "parentOrganization": "file",
    "childOrganization": "CategorySetCode",
    "sensitiveColumns": [
      "specialeducationexitreason",
      "race",
      "ideadisabilitytype",
      "age",
      "sex",
      "englishlearnerstatus"
    ],
    "frequencyColumns": ["StudentCount"]
  },
