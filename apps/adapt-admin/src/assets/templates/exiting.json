    "multiFile": false,
    "description": "The unduplicated number of students with disabilities (IDEA) who are ages 14 through 21, were in special education at the start of the reporting period and were not in special education at the end of the reporting period.",
    "pages": [
        {
            "id": "allMetrics",
            "combined": true,
            "name": "Reason for Exit",
            "title": "Reason for Exit",
            "description": "as of December 01, 2022:",
            "sections": [
                {
                    "type": "quickSummary",
                    "content": {
                        "heading": {
                            "template": "In School Year {{dataView.fields.reportingYear}}, there were {{total}} students with disabilities in your State, ages 14-21 who exited special education.",
                            "variables": {
                                "total": {
                                    "args": [
                                        "StudentCount",
                                        {
                                            "field": "ReportCode",
                                            "array": true,
                                            "value": ["C009", "c009"]
                                        },
                                        {
                                            "field": "CategorySetCode",
                                            "value": "ST1"
                                        }
                                    ],
                                    "function": "sum"
                                }
                            }
                        },
                        "sections": [
                            {
                                "body": {
                                    "template": "In the reported data, {{var_1}} of students with disabilities exited special education due to receiving a regular high school diploma.",
                                    "variables": {
                                        "var_1": {
                                            "args": [
                                                "StudentCount",
                                                {
    
                                                    "field": "specialeducationexitreason",
                                                    "value": "GHS"
                                                },
                                                {
                                                    "field": "ReportCode",
                                                    "array": true,
                                                    "value": ["C009", "c009"]
                                                },
                                                {
                                                    "field": "CategorySetCode",
                                                    "value": "ST1"
                                                }
                                            ],
                                            "function": "percentage"
                                        }
                                    }
                                },
                                          "title": ""
                            },
                            {
                                "body": {
                                    "template": "In the reported data, {{var_1}} of students with disabilities exited special education due to dropping out of school.",
                                    "variables": {
                                        "var_1": {
                                            "args": [
                                                "StudentCount",
                                                {
    
                                                    "field": "specialeducationexitreason",
                                                    "value": "DROPOUT"
                                                },
                                                {
                                                    "field": "ReportCode",
                                                    "array": true,
                                                    "value": ["C009", "c009"]
                                                },
                                                {
                                                    "field": "CategorySetCode",
                                                    "value": "ST1"
                                                }
                                            ],
                                            "function": "percentage"
                                        }
                                    }
                                },
                                          "title": ""
                            }
                        ]
                    },
                    "sectionLabel": "Quick Summary"
                },
                {
                    "type": "barChart",
                    "sectionLabel": "Reason for exit",
                    "content": {
                        "title": "Reason for exit",
                        "description": "The following represents students with disabilities in your State, ages 14-21, who exited special education for various reasons.",
                        "explainTemplate": "In the reported data, the top three reasons for exit in your State are, {{first}}, {{second}}, and {{third}}",
                        "chart": {
                            "filterOn": "x",
                            "yAxisLabel": "Child Count",
                            "xAxisLabel": "specialeducationexitreason",
                            "xAxisValue": "specialeducationexitreason",

                            "yAxisValue": "StudentCount",
                            "total": {
                                "id": "exitingTotalBarChart",
                                "function": "SUM",
                                "arguments": [
                                    {
                                        "field": "StudentCount"
                                    },
                                    {
                                        "field": "ReportCode",
                                        "array": true,
                                        "value": ["C009", "c009"]
                                    }
                                ]
                            },
                            "dataRetrievalOperations": [
                                {
                                    "id": "childCountExitingBarChart",
                                    "function": "GROUPBY_WITH_DEFAULTS",
                                    "arguments": [
                                        {
                                            "field": "func",
                                            "type": "string",
                                            "value": "sum"
                                        },
                                        {
                                            "field": "columns",
                                            "value": [
                                                "StudentCount"
                                            ],
                                            "array": true
                                        },
                                        {
                                            "field": "selectColumns",
                                            "value": [
                                                "specialeducationexitreason"
                                            ],
                                            "array": true
                                        },
                                        {
                                            "field": "limit",
                                            "type": "number"
                                        },
                                        {
                                            "field": "order",
                                            "type": "string",
                                            "array": true,
                                            "value": [
                                                "StudentCount desc"
                                            ]
                                        },
                                        {
                                            "field": "groupby",
                                            "type": "string",
                                            "array": true,
                                            "value": [
                                                "specialeducationexitreason"
                                            ]
                                        },
                                        {
                                            "field": "defaultValues",
                                            "type": "string",
                                            "array": true,
                                            "value": [
                                                "GHS",
                                                "GRADALTDPL",
                                                "RC",
                                                "RMA",
                                                "MKC",
                                                "TRAN",
                                                "DROPOUT",
                                                "D"
                                              ]
                                        },
                                        {
                                            "field": "ReportCode",
                                            "type": "string",
                                            "array": true,
                                            "value": ["C009", "c009"]
                                        },
                                        {
                                            "field": "CategorySetCode",
                                            "type": "string",
                                            "value": "ST1"
                                        }
